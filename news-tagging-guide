<script type="text/javascript">
  
  // Scroll Depth Events
  var trackEvent=0;
  window.onscroll = function(ev) {
    if ((window.innerHeight + window.scrollY) >=(( document.body.offsetHeight)/100)*50 && trackEvent==0) {

      ga('myTracker.send', {
          hitType: 'event',
          eventCategory: 'NTG article milestone',
          eventAction:'50%',
          eventLabel: 'https://ads.rwadx.com/ntg-demo/index.html',
          eventValue: 50,
          nonInteraction: true
      });
      console.log("reached to 50%");
      trackEvent=1;
    }
    if ((window.innerHeight + window.scrollY) >=(( document.body.offsetHeight)/100)*75 && trackEvent==1) {
      ga('myTracker.send', {
          hitType: 'event',
          eventCategory: 'NTG article milestone',
          eventAction:'75%',
          eventLabel: 'https://ads.rwadx.com/ntg-demo/index.html',
          eventValue: 75,
          nonInteraction: true
      });
      console.log("reached to 75%");
      trackEvent=2;
    }
  };

  // Social Sharing  Events

  document.getElementById("facebook").onclick=function(){
    ga('myTracker.send', 'event', {
        eventCategory: 'NTG social',
        eventAction: 'social share',
        eventLabel: 'facebook',
        nonInteraction: false
    });
    console.log("facebook click");
  }

  document.getElementById("twitter").onclick=function(){
    ga('myTracker.send', 'event', {
        eventCategory: 'NTG social',
        eventAction: 'social share',
        eventLabel: 'Twitter',
        nonInteraction: false
    });
    console.log("Twitter click");
  }

  document.getElementById("whatsapp").onclick=function(){
    ga('myTracker.send', 'event', {
        eventCategory: 'NTG social',
        eventAction: 'social share',
        eventLabel: 'Whatsapp',
        nonInteraction: false
    });
    console.log("Whatsapp click");
  }


  // Notifications
  document.getElementById("subsribe").onclick=function(){
    ga('myTracker.send', 'event', {
        eventCategory: 'NTG push notification',
        eventAction: 'click',
        eventLabel: 'https://ads.rwadx.com/ntg-demo/index.html',
        nonInteraction: false
    });
    console.log("Subscribe click");
    swal({
      title: "Thanks for subscribing us.",
      text: "We will stick to important stuff.",
      icon: "success",
      button: "Got it",
    });
  }


  // Add to home screen events
  document.getElementById("addtohome").onclick=function(){
    ga('myTracker.send', 'event', {
        eventCategory: 'NTG Add to HomeScreen',
        eventAction: 'add to homescreen',
        eventLabel: 'https://ads.rwadx.com/ntg-demo/index.html',
        nonInteraction: false
    });
    console.log("Addtohome click");
    swal({
      title: "Add to Home Screen",
      text: "",
      icon: "success",
      button: "OK",
    });
  }

</script>

